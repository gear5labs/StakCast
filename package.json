{
  "name": "stakcast",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "client",
    "landing_page",
    "server"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "pnpm --filter client... build && pnpm --filter stakcast_landing_page... build && pnpm --filter server... build",
    "dev:client": "pnpm --filter client dev",
    "dev:landing": "pnpm --filter stakcast_landing_page dev",
    "dev:server": "pnpm --filter server dev",
    "frontend": "cd client && npm run dev",
    "server": "cd server && npm run dev",
    "all": "concurrently \"npm run frontend\" \"npm run server\"  "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": "pnpm exec eslint --fix"
  },
  "dependencies": {
    "concurrently": "^9.2.0"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
